{
    "name": "juniora/link-preview",
    "type": "library",
    "description": "Link preview generation for PHP with Laravel support",
    "keywords": ["php","url","scraping","laravel","preview"],
    "homepage": "https://github.com/juniora/link-preview",
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Mysenko",
            "email": "denis@mysenko.com",
            "homepage": "https://www.mysenko.com"
        },
        {
            "name": "Nima HeydariNasab",
            "email": "nima.heydari79@yahoo.com",
            "homepage": "https://nimah79.ir"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^11.0",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/dom-crawler": "^7.0",
        "symfony/css-selector": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Dusterio\\LinkPreview\\": "src/"
        }
    },
    "extra":{
        "laravel": {
            "providers": [
                "Dusterio\\LinkPreview\\Integrations\\LaravelServiceProvider"
            ]
        }
    }
}
